{
   "background": {
      "scripts": [ "ext-onetab-concatenated-sources-background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/onetab.png",
         "38": "images/onetab.png"
      }
   },
   "commands": {
      "display-onetab": {
         "description": "__MSG_displayOneTab__",
         "suggested_key": {
            "default": "Alt+Shift+1"
         }
      },
      "send-current-tab-to-onetab": {
         "description": "__MSG_sendCurrentTabToOneTab__"
      }
   },
   "content_security_policy": "script-src 'self'; object-src 'self'; img-src 'self' data: https://s2.googleusercontent.com chrome://favicon;",
   "default_locale": "en",
   "description": "__MSG_manifestDescription__",
   "icons": {
      "128": "images/onetab.png",
      "16": "images/onetab.png",
      "32": "images/onetab.png",
      "48": "images/onetab.png",
      "64": "images/onetab.png"
   },
   "manifest_version": 2,
   "name": "OneTab Reborn",
   "options_page": "options.html",
   "permissions": [ "chrome://favicon/", "unlimitedStorage", "storage", "tabs", "contextMenus", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
